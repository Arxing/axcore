<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Divider">
        <attr name="divider_orientation" format="enum">
            <enum name="vertical" value="0"/>
            <enum name="horizontal" value="1"/>
        </attr>
        <attr name="divider_size" format="dimension"/>
        <attr name="divider_colors" format="string"/>
        <attr name="divider_color" format="color"/>
        <attr name="divider_colors_direction" format="enum">
            <enum name="LEFT_RIGHT" value="0x10"/>
            <enum name="RIGHT_LEFT" value="0x11"/>
            <enum name="TOP_BOTTOM" value="0x12"/>
            <enum name="BOTTOM_TOP" value="0x13"/>
            <enum name="LT_RB" value="0x14"/>
            <enum name="RB_LT" value="0x15"/>
            <enum name="LB_RT" value="0x16"/>
            <enum name="RT_LB" value="0x17"/>
        </attr>
    </declare-styleable>
</resources>